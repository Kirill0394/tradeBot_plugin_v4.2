{
  "manifest_version": 2,
  "name": "tradeBot_w_deflt_input",
  "version": "1.9",
  "description": "tradeBot for PocketOption",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon_16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png",
    "16": "icon_16.png"
  },
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  }
}
